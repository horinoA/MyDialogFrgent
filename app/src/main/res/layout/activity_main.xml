<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.horinoa.mydialogfragment.MainActivity">

    <GridLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:columnCount="2"
        android:rowCount="10"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true">

        <TextView
            android:text="null"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_alignParentStart="true"
            android:layout_column="0"
            android:layout_row="0"
            android:id="@+id/okAlertTextView" />

        <Button
            android:text="okAlert"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/okAlertButtom"
            android:layout_marginTop="11dp"
            android:layout_below="@+id/okAlertTextView"
            android:layout_column="0"
            android:layout_row="1"
            android:layout_alignParentStart="true" />

        <TextView
            android:text="null"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:id="@+id/okNoAlertTextView"
            android:layout_column="0"
            android:layout_row="2"
            android:layout_alignParentStart="true" />

        <Button
            android:text="okNoAlert"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/okNoAlertButtom"
            android:layout_column="0"
            android:layout_row="3"
            android:layout_alignParentStart="true" />

        <TextView
            android:text="null"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:id="@+id/okLaterNoAlertTextView"
            android:layout_column="0"
            android:layout_row="4"
            android:layout_alignParentStart="true" />

        <Button
            android:text="okLaterNoAlert"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/okLaterNoAlertButtom"
            android:layout_column="0"
            android:layout_row="5"
            android:layout_alignParentStart="true" />

        <TextView
            android:text="null"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:id="@+id/listAlertTextView"
            android:layout_column="0"
            android:layout_row="6"
            android:layout_alignParentStart="true" />

        <Button
            android:text="listAlert"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/listAlertButtom"
            android:layout_column="0"
            android:layout_row="7"
            android:layout_alignParentStart="true" />

        <TextView
            android:text="null"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:id="@+id/listSingleAlertTextView"
            android:layout_column="0"
            android:layout_row="8"
            android:layout_alignParentStart="true" />

        <Button
            android:text="listSingleAlert"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/listSingleAlertButtom"
            android:layout_column="0"
            android:layout_row="9"
            android:layout_alignParentStart="true" />

        <TextView
            android:text="null"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:id="@+id/listMultiAlertTextView"
            android:layout_column="1"
            android:layout_row="0"
            android:layout_alignParentStart="true" />

        <Button
            android:text="listMultiAlert"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/listMultiAlertButtom"
            android:layout_column="1"
            android:layout_row="1"
            android:layout_alignParentStart="true" />

        <TextView
            android:text="null"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:id="@+id/editTextAlertTextView"
            android:layout_column="1"
            android:layout_row="2"
            android:layout_alignParentStart="true" />

        <Button
            android:text="edittextAlert"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/editTextAlertButtom"
            android:layout_column="1"
            android:layout_row="3"
            android:layout_alignParentStart="true" />


    </GridLayout>


</RelativeLayout>
